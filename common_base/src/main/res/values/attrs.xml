<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义加载框-->
    <declare-styleable name="LVCircleRing">
        <attr name="bgColor" format="color"></attr>
        <attr name="barColor" format="color"></attr>
    </declare-styleable>


    <!-- 横向View封装 -->
    <declare-styleable name="MultipleItemView">
        <attr name="leftText" format="string"></attr>
        <attr name="leftTextSize" format="dimension"></attr>
        <attr name="leftTextColor" format="color"></attr>

        <attr name="rightText" format="string"></attr>
        <attr name="rightTextSize" format="dimension"></attr>
        <attr name="rightTextColor" format="color"></attr>

        <attr name="leftIcon" format="reference"></attr>
        <attr name="rightIcon" format="reference"></attr>

        <attr name="showTopLine" format="boolean"></attr>
        <attr name="showBottomLine" format="boolean"></attr>
        <attr name="showLeftIcon" format="boolean"></attr>
        <attr name="showRightIcon" format="boolean"></attr>
        <attr name="showEditText" format="boolean"></attr>
    </declare-styleable>

</resources>